{
  "name": "@devolutions/iron-remote-desktop-rdp",
  "author": "Nicolas Girot",
  "email": "ngirot@devolutions.net",
  "contributors": [
    "Benoit Cortier",
    "Irving Ou",
    "Vladislav Nikonov",
    "Zacharia Ellaham"
  ],
  "description": "Web Component providing agnostic implementation for Iron Wasm base client",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "npm run wasm-build && npm run pre-build && vite && npm run post-build",
    "build": "npm run wasm-build && npm run pre-build && vite build && npm run post-build",
    "build-alone": "npm run pre-build && vite build && npm run post-build",
    "pre-build": "node ./pre-build.js",
    "wasm-build": "node ./wasm-build.js",
    "post-build": "node ./post-build.js",
    "preview": "vite preview",
    "check": "tsc --noEmit",
    "check:dist": "tsc ./dist/index.d.ts --noEmit",
    "check:watch": "tsc --watch --noEmit",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint src/**",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "globals": "^16.0.0",
    "prettier": "^3.1.0",
    "tslib": "^2.4.1",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-top-level-await": "^1.2.2",
    "vite-plugin-wasm": "^3.1.0"
  },
  "dependencies": {
    "rxjs": "^6.6.7",
    "ua-parser-js": "^1.0.33"
  }
}
